public class Solution { 
    public static int[] missingAndRepeating(ArrayList<Integer> arr, int n) {
        int [] freq = new int[n+1];
        for(int i=0; i<n; i++) {
            freq[arr.get(i)]++;
        }
        int [] ans = new int[2];
        for(int i=1; i<freq.length; i++) {
            if(freq[i] > 1) {
                ans[1] = i;
            } else if (freq[i] == 0) {
                ans[0] = i;
            }
        }
        return ans;
    }

}
